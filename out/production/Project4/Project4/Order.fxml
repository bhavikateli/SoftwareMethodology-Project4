<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="427.0" prefWidth="442.0" xmlns="http://javafx.com/javafx/15.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Project4.OrderController">
    <children>
        <Text fx:id="yourOrderText" layoutX="31.0" layoutY="112.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Your Order">
            <font>
                <Font name="American Typewriter" size="31.0"/>
            </font>
        </Text>
        <Text fx:id="subtotalText" layoutX="233.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Sub-Total: "/>
        <Text fx:id="salesTaxText" layoutX="236.0" layoutY="83.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Sales Tax:"/>
        <Text fx:id="totalText" layoutX="263.0" layoutY="111.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Total:"/>
        <TextField fx:id="subtotalTextArea" layoutX="302.0" layoutY="38.0" prefHeight="27.0" prefWidth="104.0"/>
        <TextField fx:id="salesTaxTextArea" layoutX="302.0" layoutY="65.0" prefHeight="27.0" prefWidth="104.0"/>
        <TextField fx:id="totalTextArea" layoutX="302.0" layoutY="92.0" prefHeight="27.0" prefWidth="104.0"/>
        <ListView fx:id="orderListView" layoutX="31.0" layoutY="139.0" prefHeight="200.0" prefWidth="376.0"/>
        <Button fx:id="placeOrderButton" layoutX="306.0" layoutY="358.0" mnemonicParsing="false" onAction="#placeOrder"
                text="Place Order"/>
        <Button fx:id="removeSelectedItemButton" layoutX="135.0" layoutY="358.0" mnemonicParsing="false"
                onAction="#removeItem" text="Remove Selected Item"/>
    </children>
</AnchorPane>
